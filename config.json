  "visualization": {
    "radar_modes": {
      "grid": {
        "name": "Grid Mode",
        "description": "Organized grid layout preventing AP overlaps",
        "settings": {
          "grid_size": 6,
          "spacing": 60,
          "show_grid_lines": true,
          "intelligent_positioning": true
        }
      },
      "polar": {
        "name": "Polar Mode", 
        "description": "Traditional radar sweep with distance rings",
        "settings": {
          "ring_count": 6,
          "ring_spacing": 30,
          "sweep_animation": true,
          "angle_labels": true
        }
      },
      "heatmap": {
        "name": "Heatmap Mode",
        "description": "Signal strength visualization with threat colors",
        "settings": {
          "gradient_colors": ["#00ff00", "#ffff00", "#ff8800", "#ff0000"],
          "blur_radius": 20,
          "intensity_scale": 0.8
        }
      }
    },
    "colors": {
      "background": "#0a0a0a",
      "grid_lines": "#333333",
      "text": "#00ff00",
      "accent": "#00ff00",
      "critical": "#ff1744",
      "high": "#ff6d00",
      "medium": "#ff9800",
      "low": "#ffeb3b",
      "minimal": "#4caf50",
      "unknown": "#9e9e9e"
    }
  },
  "ui_layout": {
    "window_sizes": {
      "compact": {
        "width": 800,
        "height": 500
      },
      "normal": {
        "width": 1400,
        "height": 800
      },
      "fullscreen": {
        "maximize": true
      }
    },
    "panels": {
      "radar_panel": {
        "min_width": 400,
        "min_height": 400
      },
      "info_panel": {
        "min_width": 300,
        "max_width": 500
      }
    }
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "wifi_radar.log",
    "max_file_size": "10MB",
    "backup_count": 5
  }
